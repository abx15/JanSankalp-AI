# Docker Environment Variables for JanSankalp AI
# Copy this file to .env.docker and fill in your actual API keys

# AI Service API Keys
OPENAI_API_KEY=your_openai_api_key_here
GROK_API_KEY=your_grok_api_key_here  
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
COHERE_API_KEY=your_cohere_api_key_here

# Other Service Keys (these are example values - replace with your own)
ASSEMBLY_AI_API_KEY=your_assembly_ai_api_key_here
RESEND_API_KEY=your_resend_api_key_here

# ImageKit Configuration (replace with your own keys)
NEXT_PUBLIC_IMAGEKIT_PUBLIC_KEY=your_imagekit_public_key_here
IMAGEKIT_PRIVATE_KEY=your_imagekit_private_key_here
NEXT_PUBLIC_IMAGEKIT_URL_ENDPOINT=your_imagekit_url_endpoint_here

# Pusher Configuration (replace with your own keys)
NEXT_PUBLIC_PUSHER_APP_ID=your_pusher_app_id_here
NEXT_PUBLIC_PUSHER_KEY=your_pusher_key_here
PUSHER_SECRET=your_pusher_secret_here
NEXT_PUBLIC_PUSHER_CLUSTER=your_pusher_cluster_here
